bool matrixArithmeticTest()
{
    var a = int[2, 3] ((1, 2, 3), (4, 5, 6));
    var b = int[2, 3] ((11, 12, 13), (14, 15, 16));

    let sum =  std::matrix::add (a, b);
    let diff = std::matrix::subtract (b, a);

    return sum[0, 0] == 12 && sum[-1, -1] == 22
        && diff[0, 0] == 10 && diff[-1, -1] == 10;
}

